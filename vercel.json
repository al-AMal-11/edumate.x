{
    "version":2,
    "builds": [
      {
        "src": "SchoolManagement/wsgi.py",
        "use": "@vercel/python",
        "config":{"maxlambdaSize":"15mb","runtime":"python3.9"}
      },
      {
        "src":"build_files.sh",
        "use": "@vercel/static-build",
        "config":{
            "disDir":"staticfiles_build"
        }
      }

    ],

    "routes": [

      {
        "src": "/(.*)",
        "dest": "SchoolManagement/wsgi.py"
      },
      {
        "src": "/static/(.*)",
        "dest":"/static/$1"
      }
    ]
}
